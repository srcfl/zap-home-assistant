[MAIN]
ignore=tests

[MESSAGES CONTROL]
disable=
    too-many-branches,
    too-many-statements,
    too-many-return-statements,
    too-many-nested-blocks,
    too-many-positional-arguments,
    import-outside-toplevel,
    broad-exception-caught,
    no-else-return,
    too-many-locals,
    too-many-arguments

[FORMAT]
max-line-length=120
